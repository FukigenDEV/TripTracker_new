<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#b1d8ff">

    <ImageView
        android:id="@+id/pf_info_holder"
        android:layout_width="380dp"
        android:layout_height="180dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="8dp"
        android:src="@drawable/layout_bg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/pf_name_header_holder"
        android:layout_width="200dp"
        android:layout_height="24dp"
        android:src="@drawable/layout_bg_top_radius"
        app:layout_constraintEnd_toEndOf="@+id/pf_info_holder"
        app:layout_constraintTop_toTopOf="@+id/pf_info_holder" />

    <TextView
        android:id="@+id/pf_name_header"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="@string/name_header"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/pf_name_header_holder"
        app:layout_constraintStart_toStartOf="@+id/pf_name_header_holder"
        app:layout_constraintTop_toTopOf="@+id/pf_name_header_holder" />

    <TextView
        android:id="@+id/pf_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="2dp"
        android:text="@string/name"
        android:textSize="13sp"
        android:textColor="#FFF"
        app:layout_constraintStart_toStartOf="@+id/pf_name_header_holder"
        app:layout_constraintTop_toBottomOf="@+id/pf_name_header_holder" />

    <ImageView
        android:id="@+id/pf_location_header_holder"
        android:layout_width="200dp"
        android:layout_height="24dp"
        android:layout_marginTop="4dp"
        android:src="@drawable/layout_bg_rect"
        app:layout_constraintEnd_toEndOf="@+id/pf_info_holder"
        app:layout_constraintTop_toBottomOf="@+id/pf_name" />

    <TextView
        android:id="@+id/pf_location_header"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="3dp"
        android:text="@string/location_header"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/pf_location_header_holder"
        app:layout_constraintTop_toTopOf="@+id/pf_location_header_holder" />

    <TextView
        android:id="@+id/pf_location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="2dp"
        android:text="@string/location"
        android:textSize="13sp"
        android:textColor="#FFF"
        app:layout_constraintStart_toStartOf="@+id/pf_location_header_holder"
        app:layout_constraintTop_toBottomOf="@+id/pf_location_header_holder" />

    <ImageView
        android:id="@+id/pf_nationality_header_holder"
        android:layout_width="200dp"
        android:layout_height="24dp"
        android:layout_marginTop="4dp"
        android:src="@drawable/layout_bg_rect"
        app:layout_constraintEnd_toEndOf="@+id/pf_info_holder"
        app:layout_constraintTop_toBottomOf="@+id/pf_location" />

    <TextView
        android:id="@+id/pf_nationality_header"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="3dp"
        android:text="@string/nationality_header"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/pf_nationality_header_holder"
        app:layout_constraintTop_toTopOf="@+id/pf_nationality_header_holder" />

    <TextView
        android:id="@+id/pf_nationality"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="2dp"
        android:text="@string/nationality"
        android:textSize="13sp"
        android:textColor="#FFF"
        app:layout_constraintStart_toStartOf="@+id/pf_nationality_header_holder"
        app:layout_constraintTop_toBottomOf="@+id/pf_nationality_header_holder" />

    <ImageView
        android:id="@+id/pf_settings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:src="@drawable/ic_settings_black_24dp"
        app:layout_constraintBottom_toBottomOf="@+id/pf_info_holder"
        app:layout_constraintEnd_toEndOf="@+id/pf_info_holder" />

    <ImageView
        android:id="@+id/pf_memory_holder"
        android:layout_width="380dp"
        android:layout_height="36dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="8dp"
        android:src="@drawable/layout_bg_top_radius"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pf_info_holder" />

    <TextView
        android:id="@+id/pf_memory"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/memory_header"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/pf_memory_holder"
        app:layout_constraintTop_toTopOf="@+id/pf_memory_holder" />

    <ImageView
        android:id="@+id/pf_memory_content_holder"
        android:layout_width="380dp"
        android:layout_height="122dp"
        android:src="@drawable/layout_bg_content"
        app:layout_constraintStart_toStartOf="@+id/pf_memory_holder"
        app:layout_constraintTop_toBottomOf="@+id/pf_memory_holder" />

    <ImageView
        android:id="@+id/pf_memory_content_1_1"
        android:layout_width="80dp"
        android:layout_height="45dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:src="@drawable/layout_memory_preview"
        app:layout_constraintStart_toStartOf="@+id/pf_memory_content_holder"
        app:layout_constraintTop_toTopOf="@+id/pf_memory_content_holder" />

    <ImageView
        android:id="@+id/pf_memory_content_1_2"
        android:layout_width="80dp"
        android:layout_height="45dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:src="@drawable/layout_memory_preview"
        app:layout_constraintStart_toEndOf="@+id/pf_memory_content_1_1"
        app:layout_constraintTop_toTopOf="@+id/pf_memory_content_holder" />

    <ImageView
        android:id="@+id/pf_memory_content_1_3"
        android:layout_width="80dp"
        android:layout_height="45dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:src="@drawable/layout_memory_preview"
        app:layout_constraintStart_toEndOf="@+id/pf_memory_content_1_2"
        app:layout_constraintTop_toTopOf="@+id/pf_memory_content_holder" />

    <ImageView
        android:id="@+id/pf_memory_content_1_4"
        android:layout_width="80dp"
        android:layout_height="45dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:src="@drawable/layout_memory_preview"
        app:layout_constraintStart_toEndOf="@+id/pf_memory_content_1_3"
        app:layout_constraintTop_toTopOf="@+id/pf_memory_content_holder" />

    <ImageView
        android:id="@+id/pf_memory_content_2_1"
        android:layout_width="80dp"
        android:layout_height="45dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:src="@drawable/layout_memory_preview"
        app:layout_constraintStart_toStartOf="@+id/pf_memory_content_holder"
        app:layout_constraintTop_toBottomOf="@+id/pf_memory_content_1_1" />

    <ImageView
        android:id="@+id/pf_memory_content_2_2"
        android:layout_width="80dp"
        android:layout_height="45dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:src="@drawable/layout_memory_preview"
        app:layout_constraintStart_toEndOf="@+id/pf_memory_content_2_1"
        app:layout_constraintTop_toBottomOf="@+id/pf_memory_content_1_2" />

    <ImageView
        android:id="@+id/pf_memory_content_2_3"
        android:layout_width="80dp"
        android:layout_height="45dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:src="@drawable/layout_memory_preview"
        app:layout_constraintStart_toEndOf="@+id/pf_memory_content_2_2"
        app:layout_constraintTop_toBottomOf="@+id/pf_memory_content_1_3" />

    <ImageView
        android:id="@+id/pf_memory_content_2_4"
        android:layout_width="80dp"
        android:layout_height="45dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:src="@drawable/layout_memory_preview"
        app:layout_constraintStart_toEndOf="@+id/pf_memory_content_2_3"
        app:layout_constraintTop_toBottomOf="@+id/pf_memory_content_1_4" />

    <ImageView
        android:id="@+id/pf_memory_more"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="2dp"
        android:src="@drawable/ic_navigate_next_black_24dp"
        app:layout_constraintBottom_toBottomOf="@+id/pf_memory_content_2_4"
        app:layout_constraintEnd_toEndOf="@+id/pf_memory_content_2_4" />

    <ImageView
        android:id="@+id/nav_bg"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:src="@drawable/layout_bg_rect_l"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/nav_bg_1"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="10dp"
        android:src="@drawable/layout_bg_rect"
        app:layout_constraintBottom_toBottomOf="@+id/nav_bg"
        app:layout_constraintStart_toStartOf="@+id/nav_bg" />

    <ImageView
        android:id="@+id/nav_bg_2"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="24dp"
        android:src="@drawable/layout_bg_rect"
        app:layout_constraintBottom_toBottomOf="@+id/nav_bg"
        app:layout_constraintStart_toEndOf="@+id/nav_bg_1" />

    <ImageView
        android:id="@+id/nav_bg_3"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="23dp"
        android:src="@drawable/layout_bg_rect"
        app:layout_constraintBottom_toBottomOf="@+id/nav_bg"
        app:layout_constraintStart_toEndOf="@+id/nav_bg_2" />

    <ImageView
        android:id="@+id/nav_bg_4"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="24dp"
        android:src="@drawable/layout_bg_rect"
        app:layout_constraintBottom_toBottomOf="@+id/nav_bg"
        app:layout_constraintStart_toEndOf="@+id/nav_bg_3" />

    <ImageView
        android:id="@+id/nav_icon_map"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginTop="15dp"
        android:src="@drawable/ic_maps"
        app:layout_constraintEnd_toEndOf="@+id/nav_bg_1"
        app:layout_constraintStart_toStartOf="@+id/nav_bg_1"
        app:layout_constraintTop_toTopOf="@+id/nav_bg_1" />

    <ImageView
        android:id="@+id/nav_icon_timeline"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_marginTop="12dp"
        android:src="@drawable/ic_menu_black_24dp"
        app:layout_constraintEnd_toEndOf="@+id/nav_bg_2"
        app:layout_constraintStart_toStartOf="@+id/nav_bg_2"
        app:layout_constraintTop_toTopOf="@+id/nav_bg_2" />

    <ImageView
        android:id="@+id/nav_icon_camera"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_marginTop="13dp"
        android:src="@drawable/ic_photo_camera_black_24dp"
        app:layout_constraintEnd_toEndOf="@+id/nav_bg_3"
        app:layout_constraintStart_toStartOf="@+id/nav_bg_3"
        app:layout_constraintTop_toTopOf="@+id/nav_bg_3" />

    <ImageView
        android:id="@+id/nav_icon_profile"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="8dp"
        android:src="@drawable/ic_person_black_24dp"
        app:layout_constraintEnd_toEndOf="@+id/nav_bg_4"
        app:layout_constraintStart_toStartOf="@+id/nav_bg_4"
        app:layout_constraintTop_toTopOf="@+id/nav_bg_4" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="7dp"
        android:src="@drawable/ic_add_black_24dp"
        app:layout_constraintEnd_toEndOf="@+id/pf_memory_holder"
        app:layout_constraintTop_toTopOf="@+id/pf_memory_holder" />

</android.support.constraint.ConstraintLayout>